<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <EditText
        android:layout_width="wrap_content"
        android:id="@+id/ch10_2_et"
        android:layout_height="wrap_content" />
    <Button
        android:layout_width="wrap_content"
        android:text="send_broadcost"
        android:layout_height="wrap_content" />
    <Button
        android:layout_width="wrap_content"
        android:text="Ch10Activity"
        android:layout_height="wrap_content" />
    <TextView
        android:layout_width="wrap_content"
        android:id="@+id/ch10_1_tv"
        android:layout_height="wrap_content" />
    <Button
        android:layout_width="wrap_content"
        android:text="start_sub_activity"
        android:onClick="startSubActivity"
        android:layout_height="wrap_content" />
    <Button
        android:layout_width="wrap_content"
        android:text="web"
        android:onClick="web"
        android:layout_height="wrap_content" />
    <Button
        android:layout_width="wrap_content"
        android:text="contractList"
        android:onClick="contactsList"
        android:layout_height="wrap_content" />
    <Button
        android:layout_width="wrap_content"
        android:text="contractDetail"
        android:onClick="contractsDeatil"
        android:layout_height="wrap_content" />
    <Button
        android:layout_width="wrap_content"
        android:text="showMap"
        android:onClick="showMap"
        android:layout_height="wrap_content" />
    <Button
        android:layout_width="wrap_content"
        android:text="showPhoto"
        android:onClick="showPhoto"
        android:layout_height="wrap_content" />
    <Button
        android:layout_width="wrap_content"
        android:text="PICK_CONTACT"
        android:onClick="pickContact"
        android:layout_height="wrap_content" />

</LinearLayout>
